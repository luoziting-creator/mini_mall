<!--pages/detail/childcom/w-detail-swiper/w-detail-swiper.wxml-->
<!-- <text>pages/detail/childcom/w-detail-swiper/w-detail-swiper.wxml</text> -->
<view class="detail_swiper">
  <swiper class="swiper" circular autoplay interval="3000" duration="300" indicator-dots indicator-active-color="#ff5777">
    <block wx:for="{{list}}" wx:key="index">
      <swiper-item class="detail-item">
      <image src="{{item.image}}" mode="widthFix"></image>
    </swiper-item>
    </block>
  </swiper>
</view>

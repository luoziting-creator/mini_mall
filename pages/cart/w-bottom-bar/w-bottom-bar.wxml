<view class="bottom-bar">
  <view class="check-box">
    <w-check-icon icon="icon" isChecked="{{selected}}"></w-check-icon>
    <view>全选</view>
  </view>
  <view class="counter">
    合计：<text class="counter-text">￥{{price}}</text>
  </view>
  <view class="payment">去计算( {{counter}} )</view>
</view>
